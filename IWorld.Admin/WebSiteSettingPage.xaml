<UserControl xmlns:Admin="clr-namespace:IWorld.Admin" x:Class="IWorld.Admin.WebSiteSettingPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid x:Name="root" Width="912">
        <TextBlock Text="系统设置" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="18"></TextBlock>
        <Grid Name="body" Width="480" MinHeight="100"
              HorizontalAlignment="Left" VerticalAlignment="Top" Margin="105,62,0,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition Width="15"></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
        </Grid>
        <Admin:SubmitButton Text="修  改" HorizontalAlignment="Left" Margin="629,62,0,0"
                            VerticalAlignment="Top" ClickEventHandler="Edit"/>
        <Admin:SubmitButton Text="取  消" HorizontalAlignment="Left" Margin="629,92,0,0"
                            VerticalAlignment="Top" ClickEventHandler="BackToIndex"/>
    </Grid>
</UserControl>
